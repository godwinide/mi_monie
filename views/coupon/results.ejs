                <!-- HEADER DESKTOP-->
                    <%- include("../headerDesk") %>
                <!-- HEADER DESKTOP-->

    <!-- PAGE CONTAINER-->
    <div class="page-container">
        <!-- HEADER DESKTOP-->
        <!-- HEADER DESKTOP-->

        <!-- MAIN CONTENT-->
        <div class="main-content">
            <div class="">
                <div class="container-fluid">
                    <div class="row">
                        <div class="">
                            <div class="">
                                <div class="card-body">
                                    <div class="card-title">
                                        <h3 class="text-center title-2">Coupon Result.  Source: <%= source %></h3>
                                        <button class="btn btn-primary" id="print_button">
                                            <i class="fas fa-print"></i>
                                            Print
                                        </button>
                                    </div>
                                    <hr>

                                    <div id="result" class="printArea"></div>
                                    <input type="hidden" id="result-wrap" value="<%= table %>">
                                </div>
                            </div>
                        </div>
                </div>
            </div>
        </div>
    </div>


    
    <script>
        window.onload = () => {            

            const result = document.querySelector("#result");
            const resultWrap = document.querySelector("#result-wrap");
            result.innerHTML = resultWrap.value;

            const table = result.querySelector("#table");

            table.style.borderColor = "#000";

            table.classList.remove("table-mc-cyan");
            table.classList.remove("table-hover");
            
            table.classList.add("table-striped");
            table.classList.add("table-earning");

            table.querySelectorAll("thead").forEach(e => {
                e.classList.remove("lighten-3")
                e.classList.remove("purple")
            })

            table.querySelectorAll("td").forEach(e => {
                e.classList.remove("white-text")
                e.classList.remove("purple")
                e.classList.remove("lighten-2")
                e.setAttribute("contenteditable", "true");
            })
            table.querySelectorAll("th").forEach(e => {
                e.classList.remove("white-text");
                e.classList.remove("purple");
                e.classList.remove("lighten-2");
                e.setAttribute("contenteditable", "true");
            })

        }
    </script>

</div>
